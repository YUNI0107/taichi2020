;\nimport Draggable from \"gsap/Draggable\";\nimport ScrollTrigger from \"gsap/ScrollTrigger\";\ngsap.registerPlugin(ScrollTrigger, Draggable);\nimport { TimelineMax } from \"gsap\";\n\nexport default {\n  data(){\n    return{\n      all_paper:[\n        {\n          paper_session: {\n          eachtitle:{\n          num: 'D',\n          title: 'Demo Session',\n          place: \"XR Studio 光華館402室\",\n          date: \"24\",\n          time: '13:30 - 16:30'\n        },\n          session_list:[\n            'Wind Bender: Using Spherical Anemometer to Collect Weather Information for Virtual Reality',\n            '探討利用腦波訊號作為虛擬實境中隱含性互動機制之體驗-以打坐冥想體驗為例',\n            '重生Reborn',\n            '家暴防治議題與互動遊戲結合： VR 遊戲《小黃傘》',\n          ]\n          }\n        },\n        {\n          paper_session: {\n          eachtitle:{\n          num: 'D',\n          title: 'Demo Session',\n          place: \"互動系工作室 光華館404室\",\n          date: \"24\",\n          time: '13:30 - 16:30'\n        },\n          session_list:[\n            '應用於視障兒童之互動式聽覺玩具: 多感樂爆彈',\n            '視障兒童玩具設計-多感槌球',\n            'Virtual Card Game: Application of Handheld Mobile Device and Computers Wireless Communication Interaction',\n            '結合電覺與嗅覺感知之虛擬水杯',\n            '以HTC Vive Tracker 虛擬定位整合虛實空間 - 以櫥窗展示為例',\n            '基於體感的多模式互動於虛擬實境應用之設計探討',\n            'Developing a Dual-Feature Platform to Assist Students’ Course Selection Process','Doniao Gallery: Using emotion recognition system to transform emotions into images','把握機會: 利用可攜式壓力感應器及趣味 化遊戲來促進握力復健','SiLIDcon: 利用物聯網感測器來減少食物的浪費'\n          ]\n          }\n        },\n      ],\n      topList: [],\n    }\n  },\n  components:{\n    EachPart,\n    BackTop,\n  },\n  mounted(){\n    let each = document.querySelectorAll('.each-content')\n    each.forEach((item)=>{\n    this.topList.push(item.offsetTop)\n    window.addEventListener('scroll',function(){\n      var scrollTop = window.pageYOffset|| document.documentElement.scrollTop || document.body.scrollTop;\n      if(scrollTop+500 > item.offsetTop){\n        item.classList.remove('hide')\n      }\n      })\n      let scrollTop = window.pageYOffset|| document.documentElement.scrollTop || document.body.scrollTop;\n      if(scrollTop+800 > item.offsetTop){\n        item.classList.remove('hide')\n      }\n    })\n    const tl = new TimelineMax();\n    tl.from('.line', 1.5, {\n     marginLeft: '60%',\n    })\n\n    \n  }\n}\n</script>\n\n\n<style scoped>\n\n.content{\n  margin: 20px 150px;\n}\n\n.big_title{\n  display: flex;\n  align-items: center;\n  margin: 50px 0 20px 0;\n}\n\n.big_title h1{\n  display: inline-block;\n  letter-spacing: 2px;\n}\n\n.big_title .line{\n  flex: 1;\n  margin-left: 30px;\n  width: 80%;\n  height: 3px;\n  border-radius: 20px;\n  background: #06D3CC;\n}\n\n.big_title .circle{\n  width: 30px;\n  height: 30px;\n  background: #06D3CC;\n  border-radius: 50%;\n}\n\n\n@media only screen and (max-width: 800px){\n\n  .content{\n      margin: 20px 30px;\n  }  \n\n  .big_title h1{\n    font-size: 20px;\n    letter-spacing: 1px;\n  }\n\n\n  .big_title .line{\n    margin-left: 10px;\n    height: 2px;\n    border-radius: 20px;\n  }\n\n  .big_title .circle{\n    width: 10px;\n    height: 10px;\n    background: #06D3CC;\n    border-radius: 50%;\n  }\n\n\n\n}\n\n</style>\n"]}]}